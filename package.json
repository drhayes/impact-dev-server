    "start": "node ./bin/impact-dev-server"
  },
  "dependencies": {
    "optimist": "0.2.x",
    "glob": "3.1.x",
    "q": "0.9.x",
    "lodash": "~2.3.0",
    "connect": "~2.11.0"
  },
  "bugs": {
    "url": "https://github.com/drhayes/impact-dev-server/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/drhayes/impact-dev-server/raw/master/LICENSE"
    }
  ],
  "analyze": false,
  "preferGlobal": "true",
  "bin": {
    "impact-dev-server": "./bin/impact-dev-server"
  }
}
