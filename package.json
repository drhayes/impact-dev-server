{
  "name": "impact-dev-server",
  "version": "0.2.0",
  "author": "David Hayes <drhayes@gmail.com>",
  "description": "a simple zero-configuration command-line http server for ImpactJS",
  "main": "./lib/impact-dev-server",
  "repository": {
    "type": "git",
    "url": "git://github.com/drhayes/impact-dev-server.git"
  },
  "keywords": [
    "cli",
    "command",
    "impactjs"
  ],
