  "contributors": [
    {
      "name": "Sean Bohan",
      "url": "https://github.com/pixelpicosean"
    }
  ],
  "description": "A zero-configuration command-line http server for ImpactJS",
  "main": "./lib/impact-dev-server",
  "repository": {
